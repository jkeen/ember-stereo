{
  "name": "ember-stereo",
  "version": "2.0.0-semantically-released",
  "description": "The best way to play audio in your modern ember app",
  "keywords": [
    "ember-addon",
    "ember-stereo",
    "ember-hifi",
    "audio",
    "hls",
    "html5",
    "howler",
    "sound",
    "mp3",
    "api",
    "web"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jkeen/ember-stereo.git"
  },
  "license": "MIT",
  "author": "Jeff Keen",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --print-name --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --print-name --aggregate-output --continue-on-error --parallel \"lint:*:fix\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all --print-name \"lint\" \"test:*\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "semantic-release": "semantic-release",
    "format": "prettier --write \"**/*.{js,hbs,ts,tsx,md}\""
  },
  "dependencies": {
    "@embroider/macros": "^1.8.3",
    "can-autoplay": "3.0.2",
    "debug": "4.3.2",
    "ember-auto-import": "^2.4.3",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.1.1",
    "ember-concurrency": "^2.1.2",
    "ember-gesture-modifiers": "^4.0.1 || ^5.0.0",
    "ember-get-config": "^2.0",
    "ember-modal-dialog": "^4.0.0",
    "ember-modifier": "^4.0.0",
    "hls.js": "^1.1.1",
    "howler": "^2.2.1",
    "tracked-built-ins": "^3.1.0",
    "tracked-toolbox": "^2.0.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/render-modifiers": "^2.0.4",
    "@ember/test-helpers": "^2.8.1",
    "@ember/test-waiters": "^3.0.1",
    "@embroider/test-setup": "^1.8.3",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@tailwindcss/forms": "^0.3.3",
    "autoprefixer": "^10.3.4",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "3.0.0",
    "ember-cli": "~4.8.0",
    "ember-cli-addon-docs": "^5.1.0",
    "ember-cli-addon-docs-yuidoc": "^1.0.0",
    "ember-cli-bundle-analyzer": "^0.2.2",
    "ember-cli-code-coverage": "^v1.0.0-beta.8",
    "ember-cli-custom-assertions": "^0.3.0",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-postcss": "^7.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-compatibility-helpers": "^1.2.6",
    "ember-composable-helpers": "^5.0.0",
    "ember-data": "^4.4.0",
    "ember-decorators": "^6.1.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-event-helpers": "^0.1.1",
    "ember-format-json": "^0.0.4",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^6.0.0",
    "ember-resolver": "^8.0.3",
    "ember-sinon": "^5.0.0",
    "ember-source": "~4.8.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-svg-jar": "^2.3.3",
    "ember-template-lint": "^4.16.1",
    "ember-truth-helpers": "^3.0.0",
    "ember-try": "^2.0.0",
    "ember-useragent": "^0.12.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^11.1.0",
    "eslint-plugin-ember-concurrency": "^0.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "postcss-import": "^14.0.2",
    "prettier": "^2.7.1",
    "qunit": "^2.19.2",
    "qunit-dom": "^2.0.0",
    "semantic-release": "^19.0.3",
    "tailwindcss": "^2.2.7",
    "tailwindcss-theming": "^3.0.0-beta.3",
    "webpack": "^5.74.0"
  },
  "resolutions": {
    "ember-keyboard": "^7.0.1",
    "ember-modal-dialog": "^4.0.0"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "homepage": "https://ember-stereo.com",
  "volta": {
    "node": "14.18.1",
    "yarn": "1.22.11",
    "npm": "7.22.0"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "*!",
              "release": "major"
            },
            {
              "type": "feat",
              "release": "minor"
            },
            {
              "type": "build",
              "release": "patch"
            },
            {
              "type": "ci",
              "release": "patch"
            },
            {
              "type": "chore",
              "release": "patch"
            },
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            },
            {
              "type": "test",
              "release": "patch"
            }
          ],
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          }
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogTitle": "ember-stereo changelog",
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ],
          "message": "${nextRelease.version} CHANGELOG [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ],
    "debug": false,
    "dryRun": false
  }
}
